	<h2 style="text-align:center;margin-top:60px">User Login</h2>
	<p id='notice' style='background:red;text-align:center;'><%= notice %></p>
	<div id="login">
		<%= form_tag("/sessions", :class =>"well form-inline") do %>
			<%= label_tag :email %><%= email_field_tag :email %>
			<%= label_tag :password %><%= password_field_tag :password %>
			<%= submit_tag "Sign In" %></br>
		<% end %>
	</div>


